{
  "expo": {
    "name": "Space Explorer",
    "slug": "Space-Explorer",
    "description": "This app was created for educational purposes and has no affiliation with NASA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rocket-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/app-welcome-screen-final.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
